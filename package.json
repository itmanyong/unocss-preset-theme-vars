{
  "name": "unocss-preset-theme-vars",
  "type": "module",
  "version": "0.2.1",
  "packageManager": "pnpm@8.12.0",
  "description": "基于css变量方案的unocss主题配置动态化",
  "author": "Itmanyong <itmanyong@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/itmanyong/unocss-preset-theme-vars#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itmanyong/unocss-preset-theme-vars.git"
  },
  "bugs": {
    "url": "https://github.com/itmanyong/unocss-preset-theme-vars/issues"
  },
  "keywords": [
    "itmanyong",
    "unocss",
    "unocss-preset",
    "unocss-theme-var",
    "unocss-preset-theme-vars"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
