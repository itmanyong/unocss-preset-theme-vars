    "@ant-design/colors": "7.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.4.3",
    "@types/node": "20.10.4",
    "@unocss/preset-uno": "0.58.0",
    "bumpp": "9.2.0",
    "eslint": "8.55.0",
    "typescript": "5.3.3",
    "unbuild": "2.0.0"
  }
